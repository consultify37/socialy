{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", ".d.ts", "**/*.tsx", "components/Home/Why-Us/Cart.js", "components/Home/OurClients/OurClients.js", "components/Home/carousel-home.js", "components/fonduri/index.js", "pages/despre/index.tsx"],
  "exclude": ["node_modules"]
}
